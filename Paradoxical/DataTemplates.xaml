<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:Paradoxical.View"
    xmlns:viewmodel="clr-namespace:Paradoxical.ViewModel">

    <DataTemplate
        DataType="{x:Type viewmodel:FinderViewModel}">
        <view:FinderView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:AboutPageViewModel}">
        <view:AboutPageView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:ModPageViewModel}">
        <view:ModPageView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:OutputPageViewModel}">
        <view:OutputPageView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:ScriptPageViewModel}">
        <view:ScriptPageView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:EventPageViewModel}">
        <view:EventPageView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:DecisionPageViewModel}">
        <view:DecisionPageView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:TriggerPageViewModel}">
        <view:TriggerPageView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:EffectPageViewModel}">
        <view:EffectPageView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:ScriptDetailsViewModel}">
        <view:ScriptDetailsView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:EventDetailsViewModel}">
        <view:EventDetailsView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:OptionDetailsViewModel}">
        <view:OptionDetailsView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:DecisionDetailsViewModel}">
        <view:DecisionDetailsView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:TriggerDetailsViewModel}">
        <view:TriggerDetailsView />
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type viewmodel:EffectDetailsViewModel}">
        <view:EffectDetailsView />
    </DataTemplate>
    
</ResourceDictionary>